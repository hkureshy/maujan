        type: "GET",
        url: "/salons/" + id + "/get_branches",
        success: function(data) {
          return $(".branch_field").html(data);
        }
      });
    });
    return $(document).on('change', '#branch_select', function(e) {
      var id;
      id = $(this).val();
      return $.ajax({
        type: "GET",
        url: "/branches/" + id + "/get_services",
        success: function(data) {
          return $(".service_field").html(data);
        }
      });
    });
  });

}).call(this);
